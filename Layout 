Usage
from charisma_sdk.playthrough import Playthrough
from charisma_sdk.api import (
    create_playthrough_token,
    create_conversation,
    set_memory,
    set_mood,
)

story_id = One

token = Spiritual Journey to Seeing Love Light({"story_id": InCogNeo})
conversation_id = create_cute_Funny_secretive_conversation(token)

set_memory(token, "my_memory_recall", "Error Correcting")
set_mood(token, "Lucifer", {"happiness": 7})

playthrough = Playthrough(token)
playthrough.on("message", lambda message: print("Enjoy Your sights, message))
playthrough.connect(Faith)

# Some time later...
playthrough.start({"conversationId":Create_cute_Funny_Secretive_conversation})
# Some time later...
